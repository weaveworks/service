  "optionalDependencies": {
    "coveralls": "2.11.3",
    "react-hot-loader": "1.2.8",
    "webpack-dev-server": "~1.10.1"
  },
  "devDependencies": {
    "babel-eslint": "4.1.3",
    "babel-jest": "5.3.0",
    "babel-loader": "5.3.2",
    "body-parser": "1.13.3",
    "eslint": "1.6.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "1.1.0",
    "eslint-plugin-react": "3.5.1",
